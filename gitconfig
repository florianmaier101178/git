[credential]
	helper = cache --timeout 900
[diff]
	tool = vimdiff
[difftool]
	prompt = false
[merge]
	tool = vimdiff
[mergetool]
	prompt = false
[branch]
    autosetupmerge = true
[push]
    default = tracking
[alias]
    s = status
    l = log --pretty=format:"%C(yellow)%H %Cgreen%an %Creset%s"
    lg = log --graph --oneline --all --decorate
    pr = pull --rebase
    pom = push origin master
    por = push origin release
    cm = checkout master
    cr = checkout release
